const board = [];

for (let i = 0; i < 9; i++) {
  const row = [];
  for (let j = 0; j < 9; j++) {
    const cell = {
      id: `cell-${i}-${j}`,
      value: 0,
      isEditable: true,
      isHighlighted: false,
      color: ""
    };
    row.push(cell);
  }
  board.push(row);
}

let sudoku = sudoku.generate("medium");
let sudokuGrid = sudoku.board_string_to_grid(sudoku);